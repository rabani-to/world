{
  "name": "@radish-la/world-auth",
  "version": "0.0.1-alpha.18",
  "description": "Extending World Auth SDK for React",
  "keywords": [
    "worldchain",
    "worldcoin",
    "world"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rabani-to/world",
    "directory": "packages/world-auth"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsup && rm dist/*.d.mts"
  },
  "author": "Denny Portillo <d3portillo@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@worldcoin/minikit-js": "^1.8.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@worldcoin/minikit-js": "^1.8.0",
    "jotai": "^2.12.2",
    "react": "18",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
