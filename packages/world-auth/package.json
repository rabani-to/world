{
  "name": "@radish-la/world-auth",
  "version": "0.0.1-alpha.1",
  "description": "Extending World Auth SDK for React",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
