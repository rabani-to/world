  "license": "MIT",
  "peerDependencies": {
    "@worldcoin/minikit-js": "^1.8.0",
    "jotai": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@worldcoin/minikit-js": "^1.8.0",
    "jotai": "^2.12.2",
    "react": "18",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
